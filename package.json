{
  "name": "boilerplate-based-on-razzle",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "razzle start --type=spa",
    "build": "razzle build --type=spa",
    "start:prod": "serve -s build/public",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "effector": "^21.5.0",
    "effector-react": "^21.0.6",
    "razzle-plugin-typescript": "^3.3.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": ">=4",
    "lint-staged": "^10.4.2",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "razzle": "^3.3.7",
    "razzle-plugin-eslint": "^3.3.7",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, ts, tsx}": [
      "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
      "prettier --write ."
    ]
  }
}
